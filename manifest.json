{
  "manifest_version": 2,
  "name": "Pull requests templates",
  "version": "0.0.3-beta.15",
  "description": "Have multiple pull requests templates at hand.",
  "permissions": [
    "active_tab",
    "storage"
  ],
  "minimum_chrome_version": "61",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": []
  },
  "content_scripts": [],
  "web_accessible_resources": []
}